{
  "name": "reactjs-vite-tailwindcss-boilerplate",
  "description": "This is a boilerplate build with Vite, React 18, TypeScript, Vitest, Testing Library, TailwindCSS 3, Eslint and Prettier.",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Jo√£o Paulo Moraes",
    "email": "joaopaulomoraes@outlook.com",
    "url": "https://github.com/joaopaulomoraes"
  },
  "bugs": {
    "url": "https://github.com/joaopaulomoraes/reactjs-vite-tailwindcss-boilerplate/issues",
    "email": "joaopaulomoraes@outlook.com"
  },
  "scripts": {
    "dev": "vite --mode development",
    "build": "tsc && vite build --mode development",
    "build:production": "tsc && vite build --mode production",
    "serve": "vite preview",
    "test": "vitest",
    "test:run": "vitest run --no-color",
    "test:ui": "vitest --ui",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "generate": "openapi-typescript ../daftarproperti/storage/api-docs/api-docs.yaml --output src/api/generated.d.ts",
    "postinstall": "patch-package",
    "knip": "knip --exclude=devDependencies"
  },
  "eslintIgnore": [
    "src/api/generated.d.ts"
  ],
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.0",
    "@googlemaps/react-wrapper": "^1.1.35",
    "@heroicons/react": "^2.1.1",
    "@hookform/resolvers": "^3.3.4",
    "@material-tailwind/react": "2.1.8",
    "@tanstack/react-query": "^5.17.19",
    "axios": "^1.6.5",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "lodash": "^4.17.21",
    "openapi-typescript": "^6.7.3",
    "react": "^18.2.0",
    "react-datepicker": "^7.3.0",
    "react-dom": "^18.2.0",
    "react-draggable-bottom-sheet": "^1.15.0",
    "react-ga4": "^2.1.0",
    "react-hook-form": "^7.49.3",
    "react-image": "^4.1.0",
    "react-intersection-observer": "^9.8.1",
    "react-qr-code": "^2.0.15",
    "react-router-dom": "^6.21.1",
    "react-select": "^5.8.0",
    "react-toastify": "^10.0.4",
    "swiper": "^11.0.5",
    "tailwind-scrollbar": "^3.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/google.maps": "^3.55.10",
    "@types/lodash": "^4.17.4",
    "@types/node": "^22.7.4",
    "@types/react": "18.2.42",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "happy-dom": "^12.10.3",
    "knip": "^5.30.6",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.32",
    "prettier": "3.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.6.2",
    "vite": "^5.0.4",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  }
}
