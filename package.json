    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "happy-dom": "^12.10.3",
    "postcss": "^8.4.32",
    "prettier": "3.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2",
